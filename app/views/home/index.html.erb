<% if current_user.role.name == "Admin" %>
  <% if Operator.all.count < 1 %>
    <%= render "operators/form" %>
  <% else %>
    <%= render "home/admin" %>
  <%end%>
<% elsif current_user.role.name == "Customer"%>
 <%= render "home/customer" %>
<% elsif current_user.role.name == "Operator"%>
	<% if Operator.all.count < 1 %>
	  <%= render "operators/form" %>
	<% else %>
	 <%= render "home/operator" %>
	<% end%>
<% end%>